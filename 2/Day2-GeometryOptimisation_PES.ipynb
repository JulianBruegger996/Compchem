   "source": [
    "# You can read the trajectory file to get the positions of the atoms at each step of the optimization:\n",
    "from ase.io.trajectory import Trajectory\n",
    "\n",
    "traj = Trajectory('optimierung.traj')\n",
    "for i, atoms in enumerate(traj):\n",
    "    dist1 = atoms.get_distance(0,1)\n",
    "    dist2 = atoms.get_distance(0,2)\n",
    "    dist3 = atoms.get_distance(1,2)\n",
    "    energies = (water.get_total_energy())\n",
    "    angle = water.get_angle(1,2)\n",
    "    print(f\"Iteration {i+1}: {round(energies, 5)}eV for {round(dist1,5)}A {round(dist2,5)}A {round(dist3,5)}A {round(angle, 5)}\")\n",
    "\n",
    "\n",
    "    "
